#include <stdio.h>
#include <stdlib.h>

struct CD {
    char titulo[100];
    char artista[50];
    int num_canciones;
    int anio;
    float precio;
};

int main() {
    struct CD miCD;

    printf("Título: ");
    scanf("%s", miCD.titulo);

    printf("Artista: ");
    scanf("%s", miCD.artista);

    printf("Número de canciones: ");
    scanf("%d", &miCD.num_canciones);

    printf("Año: ");
    scanf("%d", &miCD.anio);

    printf("Precio: ");
    scanf("%f", &miCD.precio);

    return 0;
}