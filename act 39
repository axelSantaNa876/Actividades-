#include <stdio.h>
#include <string.h>

int main() {
    char palabra1[1];
    char palabra2[2];
    char resultado[3] = ""; 
    printf("Ingresa la primera palabra: ");
    scanf("%s", palabra1);

    printf("Ingresa la segunda palabra: ");
    scanf("%s", palabra2);
    strcat(resultado, palabra1);
int N = strlen(palabra1);
    for (int i = 0; i < N; i++) {
        strcat(resultado, palabra2);
    }
    printf("Palabra final: %s\n", resultado);

    return 0;
}